    "profile_config": {
        "gfx_driver": "All open-source",
        "greeter": null,
        "profile": {
            "custom_settings": {},
            "details": [],
            "main": "Xorg"
        }
    },
    "script": "guided",
    "silent": false,
    "skip_ntp": false,
    "skip_version_check": false,
    "swap": true,
    "timezone": "Europe/Brussels",
    "uki": false,
    "version": "2.7.1"
