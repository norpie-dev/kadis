{
    "additional-repositories": [
        "multilib"
    ],
    "archinstall-language": "English",
    "bootloader": "Grub",
    "config_version": "2.7.1",
    "debug": false,
    "hostname": "HOSTNAME",
    "kernels": [
        "linux-lts"
    ],
    "locale_config": {
        "kb_layout": "us",
        "sys_enc": "UTF-8",
        "sys_lang": "en_US"
    },
    "mirror_config": {
        "custom_mirrors": [],
        "mirror_regions": {
            "Belgium": [
                "http://mirror.tiguinet.net/arch/$repo/os/$arch",
